<form method ="POST" autocomplete="off"  action="/thankyou">
	<div class="addstyle">
  <div class="form-group">
    <label for="formGroupExampleInput" class="tag">First Name : <span class="glyphicon glyphicon-user"></span></label>
    <input type="text" class="form-control"  id="formGroupExampleInput" placeholder="Frist Name" name="FirstName" required>
  </div>
  <div class="form-group">
    <label for="formGroupExampleInput" class="tag">Last Name : <span class="glyphicon glyphicon-user"></span></label>
    <input type="text" class="form-control"  id="formGroupExampleInput2" placeholder="Last Name" name="LastName" required>
  </div>
  <div class="form-group">
    <label for="formGroupExampleInput" class="tag">Email Id : <span class="glyphicon glyphicon-envelope"></span></label>
    <input type="email" class="form-control" id="formGroupExampleInput" placeholder="Email Id" name="Email" required>
  </div>
  <div class="form-group">
    <label for="formGroupExampleInput" class="tag">Date Of Birth : <span class="glyphicon glyphicon-calendar"></label>
    <br>
    <input type="date" class="form-control"  id="formGroupExampleInput" name="DOB"  required>
  </div>
  <div class="form-group">
    <label for="formGroupExampleInput" class="tag">Phone Number : <span class="glyphicon glyphicon-phone"></span></label>
    <input type="text" class="form-control"  id="formGroupExampleInput" placeholder="Phone Number" name="PhNumber" required>
  </div>
  <button class="btn btn-primary" type="submit" >Submit Form</button>

</div>
</form>
